{
  "name": "expresszz",
  "version": "0.2.1",
  "main": "main.js",
  "author": "Adele Bendayan",
  "license": "MIT",
  "scripts": {
    "example:simple": "nodemon examples/oneservice.js",
    "lint": "eslint",
    "test": "jest --ci",
    "test:dev": "jest --watch",
    "test:ci": "jest --ci --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/abendayan/expresszz.git"
  },
  "keywords": [
    "nodejs",
    "express",
    "service",
    "redis"
  ],
  "jest": {
    "testEnvironment": "node",
    "coverageReporters": [
      "json-summary"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/examples/",
      "/tests/"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "service.js"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 65
      }
    }
  },
  "dependencies": {
    "amqplib": "0.8.0",
    "body-parser": "1.19.2",
    "connect-redis": "6.1.1",
    "cookie-parser": "1.4.6",
    "express": "4.17.3",
    "express-rate-limit": "6.3.0",
    "express-session": "1.17.2",
    "express-winston": "4.2.0",
    "http-proxy-middleware": "2.0.3",
    "network": "0.6.1",
    "redis": "4.0.3",
    "winston": "3.6.0"
  },
  "devDependencies": {
    "eslint": "8.9.0",
    "jest": "27.5.1",
    "nodemon": "2.0.15",
    "supertest": "6.2.2"
  }
}
